
<div class="col-md-offset-1 col-md-6">
<h2> All User</h2>

<table class="table table-striped"> 
<tr>
    <th>Name</th>
    <th>Email</th>
    <th>Created</th>
    <th>Edit</th>
    <th>Delete</th>
</tr>

<% _.each(users, function(user){ %>
    <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= user.createdAt %></td>
        <td><a href="/user/edit/<%= user.id %>">Edit</a></td>
        <td><a href="/user/destroy/<%= user.id %>">Delete</a></td>
    </tr>
<% }) %>

</table>
</div>